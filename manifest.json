{
     "manifest_version": 2,
    "name": "SRCM Extension",
     "description": "Chrome extension for SRCM",
     "author": "SR Global Solutions",
     "version": "1",
     "permissions": ["tabs"],
     "icons": {
       "16": "images/icon16.png",
       "32": "images/icon32.png",
       "48": "images/icon48.png",
       "128": "images/icon128.png"
     },
     "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["scripts/cheesify.js"]
      }
    ],
     "browser_action": {
      "default_popup": "popup.html",
      "default_title": "My first Chrome Extension"
    }
}


